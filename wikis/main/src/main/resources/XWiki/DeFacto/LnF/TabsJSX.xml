    <property>
      <code>require(['jquery', 'bootstrap'], function($) {
  'use strict';
  $('.nav-tabs a').click(function (e) {
    e.preventDefault();
    $(this).tab('show');
  });

  // Reflect the currently selected tab in the hash value without scrolling to the id
  $(".nav-tabs &gt; li &gt; a").on("shown.bs.tab", function(e) {
    var id = $(e.target).attr("href");
    if(history.pushState) {
      history.replaceState(null, null, id);
    } else {
      location.hash = id;
    }
  });

  if(location.hash) {
    var hash = decodeURIComponent(location.hash);
    var tab = $('.nav-tabs a[href="' + hash + '"]');
    if (tab !== undefined &amp;&amp; tab.tab !== undefined) {
      tab.tab('show'); // Select tab by name
    }
  }
});</code>
