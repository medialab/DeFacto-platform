    <property>
      <content>## Customized footer
#if($isInServletMode)## Visible only in a page
  &lt;div id="footerglobal"&gt;
    &lt;div class="container-fluid"&gt;
      &lt;div class="row"&gt;
        &lt;div class="col-xs-12 col-sm-3 text-left"&gt;
          &lt;h2&gt;De Facto&lt;/h2&gt;
          &lt;ul&gt;
            &lt;li&gt;&lt;a href="$xwiki.getURL('A-propos.WebHome')"&gt;À propos&lt;/a&gt;&lt;/li&gt;
            &lt;li&gt;&lt;a href="$xwiki.getURL('Mentions-legales.WebHome')"&gt;Mentions légales&lt;/a&gt;&lt;/li&gt;
            &lt;li&gt;&lt;a href="mailto:contact@defacto-observatoire.fr"&gt;Contact&lt;/a&gt;&lt;/li&gt;
            #if ($isGuest)
            &lt;li&gt;&lt;a href="$xwiki.getURL('XWiki.XWikiLogin', 'login')"&gt;Se connecter&lt;/a&gt;&lt;/li&gt;
            #end
          &lt;/ul&gt;
        &lt;/div&gt;
        &lt;div class="col-xs-12 col-sm-3 text-left"&gt;
          &lt;h2&gt;Le réseau EDMO&lt;/h2&gt;
          &lt;ul&gt;
            &lt;li&gt;&lt;a href="https://edmo.eu/" target="_blank"&gt;EDMO.eu&lt;/a&gt;&lt;/li&gt;
            &lt;li&gt;Hub 1&lt;/li&gt;
            &lt;li&gt;Hub 2&lt;/li&gt;
            &lt;li&gt;Hub 3&lt;/li&gt;
          &lt;/ul&gt;
        &lt;/div&gt;
        &lt;div class="col-xs-12 col-sm-3 text-left"&gt;
          &lt;h2&gt;Suivez-nous&lt;/h2&gt;
          &lt;ul&gt;
            &lt;li&gt;Twitter&lt;/li&gt;
            &lt;li&gt;Newsletter&lt;/li&gt;
            &lt;li&gt;RSS : 
              &lt;a href="$xwiki.getURL('XWiki.DeFacto.Rss', 'get', 'outputSyntax=plain')"&gt;Site&lt;/a&gt; - 
              &lt;a href="$xwiki.getURL('XWiki.DeFacto.FactCheck.Rss', 'get', 'outputSyntax=plain')"&gt;Fact-checks&lt;/a&gt;&lt;/li&gt;
          &lt;/ul&gt;
        &lt;/div&gt;
        &lt;div class="col-xs-12 col-sm-3 text-left defacto-footer-ec"&gt;
          &lt;img src="$xwiki.getAttachmentURL('XWiki.DeFacto.LnF.Logos', 'logo-ce-vertical-fr-neg-lr.png')"/&gt;
          &lt;p&gt;De Facto est un projet cofinancé par la Commission Européeenne.&lt;/p&gt;
        &lt;/div&gt;
      &lt;/div&gt;
    &lt;/div&gt;
  &lt;/div&gt;
#end</content>
